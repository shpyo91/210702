<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springboot.domain.BoardVO">
    <select id="selectlist" parameterType="com.example.springboot.domain.BoardVO" resultType="com.example.springboot.domain.BoardVO">
     <![CDATA[
        SELECT
            bno,
            title,
            content,
            regDate,
            viewCnt
        FROM
            tab_board
        ]]>
